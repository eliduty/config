    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^39.0.1"
  },
  "devDependencies": {
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.9",
