{
  "name": "@eliduty/eslint-config",
  "version": "3.0.0",
  "description": "This is the configurable eslint standard",
  "type": "module",
  "keywords": [
    "config",
    "eslint",
    "format",
    "flat config"
  ],
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/@eliduty/eslint-config",
  "author": {
    "name": "eliduty",
    "email": "eliduty@qq.com"
  },
  "contributors": [
    {
      "name": "eliduty",
      "email": "eliduty@qq.com",
      "url": "https://github.com/eliduty"
    }
  ],
  "files": [
    "dist/*"
  ],
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "release": "release-it"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@unocss/eslint-plugin": "^0.64.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.4.2",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-n": "^17.13.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.31.0",
    "globals": "^15.12.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "typescript-eslint": "8.0.0-alpha.31",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.15.0"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
